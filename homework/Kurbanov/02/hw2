#include "numbers.dat"
#include <iostream>

int check(int a)
{
	int i;
	if (a == 2)
		return 1;
	if (a == 0 || a == 1 || a % 2 == 0)
		return 0;
	for (i = 3; i*i <= a && a % i; i += 2)
		;
	return i * i > a;
}

int firstIndex(int pos, const int *data, int s)
{
	
		for (int i = 0; i < s; i++)
			if (data[i] == pos)
				return i;
		return s;
}

int secondIndex(int pos, const int *data, int s)
{
	for (int i = s; i > -1; i--)
		if (data[i] == pos)
			return i;
	return -1;
}

int main(int argc, char** argv)
{
	int count;
	if (argc < 3 || argc % 2 == 0) return -1;
	for (int i = 1; i < argc; i += 2)
	{
		count = 0;
		for (int j = firstIndex(std::atoi(argv[i]), Data, Size); j <= secondIndex(std::atoi(argv[i + 1]), Data, Size); j++)
			count += check(Data[j]);
		std::cout << count << std::endl;
	}
	return 0;
}
